{
    "name": "MyController",
    "version": "1.5.0-rc",
    "slug": "mycontroller",
    "description": "Run MyController.org as hassio addon",
    "url": "https://github.com/dianlight/hassio-addons/tree/master/mycontroller",
    "arch_": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "arch": ["armv7"],
    "startup": "services",
    "boot": "auto",
    "webui": "http://[HOST]:[PORT:8000]/",
    "init": false,
    "host_network": true,
    "map": ["config:rw", "ssl", "share:rw"],
    "options": {},
    "schema": {},
    "image": "dianlight/{arch}-addon-mycontroller",
    "privileged": [],
    "full_access": false,
    "ports": {
        "8000/tcp": 8000
    },
    "ports_description": {
        "8000/tcp": "Http communication port"
    },
    "auto_uart": true,
    "ingress": false,
    "ingress_port": 8000,
    "panel_icon": "mdi:puzzle",
    "panel_title": "MyController",
    "panel_admin": true,
    "stage": "experimental",
    "homeassistant": "0.98"
}